    "@babel/register": "^7.9.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.0",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.1",
